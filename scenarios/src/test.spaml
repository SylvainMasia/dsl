Program Scenario1 initialState off {
    pluggedElements
    Actuator red_led 1
    Actuator buzzer 2
    Sensor button 8

    states
    off {
        red_led = LOW
        buzzer = LOW
        button = HIGH -> on
    }
    on {
        red_led = HIGH
        buzzer = HIGH
        button = LOW -> off
    }
}