Program dual_check_alarm initialState off {
    pluggedElements
    Actuator red_led 1
    Actuator buzzer 2
    SensorDigital button 3
    SensorDigital button2 4

    states
    off {
        red_led = LOW
        buzzer = LOW
        AND(button = HIGH, button2 = HIGH) -> on 
    }
    on {
        red_led = HIGH
        buzzer = HIGH
        OR(button = LOW, button2 = LOW) -> off 
    }
}